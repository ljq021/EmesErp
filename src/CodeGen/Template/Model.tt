<#@ template language="C#" hostSpecific="true" debug="false" #>
<#@ output extension=".cs" #>
<#
        DefaultHost host = (DefaultHost)(Host);
        var designer = host.Designer;
#>
using Emes.Core;
using Emes.Core.Models;
using System.ComponentModel.DataAnnotations;

namespace Emes.Erp.<#=designer.Module#>.Models
{
    /// <summary>
    /// <#=designer.Name#>
    /// <#=designer.Desc#>
    /// </summary>
    public class <#=designer.Code#>: EntityBase<#= designer.AggregateRoot ?", IAggregateRoot":""#>
    {
         <# foreach (var f in designer.Fields)
  { #>/// <summary>
        /// <#= f.Name #>
        /// <#= f.Desc #>
        /// </summary>
        <#= f.Required ?"[Required(ErrorMessage = \""+f.Name+"是必填项\")]":""#>
        public <#= f.DataTypeValue#> <#= f.Code #> { get; set; }
<# } #>
    }
}

